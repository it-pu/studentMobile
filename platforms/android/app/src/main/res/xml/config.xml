<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.studentpu" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="BackgroundMode">
        <param name="android-package" value="de.appplant.cordova.plugin.background.BackgroundMode" />
    </feature>
    <feature name="Badge">
        <param name="android-package" value="de.appplant.cordova.plugin.badge.Badge" />
    </feature>
    <name>StudentPU</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="template/login.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="res/icon/android/ic_launcher_mdpi.png" />
    <icon density="hdpi" src="res/icon/android/ic_launcher_hdpi.png" />
    <icon density="xhdpi" src="res/icon/android/ic_launcher_xhdpi.png" />
    <icon density="xxhdpi" src="res/icon/android/ic_launcher_xxhdpi.png" />
    <resource-file src="res/my_notification_icon/android/drawable-mdpi/ic_stat_logo_pu_notif.png" target="app/src/main/res/drawable-mdpi/ic_stat_logo_pu_notif.png" />
    <resource-file src="res/my_notification_icon/android/drawable-hdpi/ic_stat_logo_pu_notif.png" target="app/src/main/res/drawable-hdpi/ic_stat_logo_pu_notif.png" />
    <resource-file src="res/my_notification_icon/android/drawable-xhdpi/ic_stat_logo_pu_notif.png" target="app/src/main/res/drawable-xhdpi/ic_stat_logo_pu_notif.png" />
    <resource-file src="res/my_notification_icon/android/drawable-xxhdpi/ic_stat_logo_pu_notif.png" target="app/src/main/res/drawable-xxhdpi/ic_stat_logo_pu_notif.png" />
    <resource-file src="res/my_notification_icon/android/drawable-xxxhdpi/ic_stat_logo_pu_notif.png" target="app/src/main/res/drawable-xxxhdpi/ic_stat_logo_pu_notif.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="KeepRunning" value="true" />
    <preference name="AndroidLaunchMode" value="singleInstance" />
</widget>
